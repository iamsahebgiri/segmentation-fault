    "react-dom": "^18.1.0",
    "react-hook-form": "^7.31.3",
    "react-hot-toast": "^2.2.0",
    "react-query": "^3.37.0",
    "start-server-and-test": "^1.12.0",
    "superjson": "^1.7.4",
    "zod": "^3.0.0"
